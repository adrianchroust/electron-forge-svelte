{
	"name": "electron-svelte",
	"productName": "Electron-Forge-Svelte",
	"version": "1.0.0",
	"description": "My Electron-Forge-Svelte Application",
	"author": "Adrian Chroust",
	"main": ".webpack/main",
	"scripts": {
		"clean": "rimraf out .webpack",
		"format": "prettier -w .",
		"format:staged": "pretty-quick --staged",
		"lint": "eslint --ext .js,.ts . && svelte-check",
		"start": "electron-forge start",
		"package": "electron-forge package",
		"make": "electron-forge make",
		"publish": "electron-forge publish"
	},
	"keywords": [
		"electron",
		"svelte"
	],
	"license": "MIT",
	"husky": {
		"hooks": {
			"pre-commit": "npm run format:staged",
			"pre-push": "npm run lint"
		}
	},
	"devDependencies": {
		"@electron-forge/cli": "^6.0.0-beta.52",
		"@electron-forge/maker-deb": "^6.0.0-beta.52",
		"@electron-forge/maker-dmg": "^6.0.0-beta.54",
		"@electron-forge/maker-rpm": "^6.0.0-beta.52",
		"@electron-forge/maker-squirrel": "^6.0.0-beta.52",
		"@electron-forge/maker-zip": "^6.0.0-beta.52",
		"@electron-forge/plugin-webpack": "^6.0.0-beta.52",
		"@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
		"@rollup/plugin-typescript": "^4.0.0",
		"@tsconfig/svelte": "^1.0.10",
		"@typescript-eslint/eslint-plugin": "^2.34.0",
		"@typescript-eslint/parser": "^2.34.0",
		"css-loader": "^3.6.0",
		"electron": "9.2.0",
		"eslint": "^6.8.0",
		"eslint-config-prettier": "^6.13.0",
		"eslint-plugin-svelte3": "^2.7.3",
		"fork-ts-checker-webpack-plugin": "^3.1.1",
		"husky": "^4.3.0",
		"node-loader": "^0.6.0",
		"prettier": "^2.1.2",
		"prettier-plugin-svelte": "^1.4.1",
		"pretty-quick": "^3.1.0",
		"rimraf": "^3.0.2",
		"sass": "^1.26.10",
		"sass-loader": "^9.0.3",
		"style-loader": "^0.23.1",
		"svelte": "^3.24.1",
		"svelte-check": "^1.0.22",
		"svelte-loader": "^2.13.6",
		"svelte-preprocess": "^4.1.1",
		"ts-loader": "^6.2.2",
		"tslib": "^2.0.0",
		"typescript": "^3.9.7",
		"webpack": "^4.44.1"
	},
	"dependencies": {
		"electron-squirrel-startup": "^1.0.0"
	}
}
